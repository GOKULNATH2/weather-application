    var _this = this;
    Object(_util__WEBPACK_IMPORTED_MODULE_1__["getJSON"])(
      this.options.serviceUrl + '/autocomplete',
      leaflet__WEBPACK_IMPORTED_MODULE_0___default.a.extend(
